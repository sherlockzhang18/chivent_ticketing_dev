{
    "builds": [
      {
        "src": "chivent_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.10"
        }
      }
    ],
    "routes": [
      { 
        "src": "/static/(.*)", 
        "dest": "/static/$1", 
        "headers": { "cache-control": "public, max-age=31536000, immutable" } 
      },
      { 
        "src": "/(.*)", 
        "dest": "chivent_project/wsgi.py" 
      }
    ]
  }
  